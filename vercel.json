{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    },
    {
      "src": "./*.html",
      "use": "@vercel/static"
    },
    {
      "src": "./style.css",
      "use": "@vercel/static"
    },
    {
      "src": "./script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/news",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
